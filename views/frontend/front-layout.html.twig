{% extends 'layout.html.twig' %}
{# 前台如何布局？ #}
{% block css_block %}
	<link rel="stylesheet" type="text/css" href="/assets/libs/bootstrap/4.0.0-alpha.5/css/bootstrap{% if not sys_param('debug')|default(false) %}.min{% endif %}.css?ver={{ver|default(1)}}">
	<link rel="stylesheet" type="text/css" href="/assets/libs/bootstrap/4.0.0-alpha.5/css/bootstrap-flex{% if not sys_param('debug')|default(false) %}.min{% endif %}.css?ver={{ver|default(1)}}">
	<link rel="stylesheet" type="text/css" href="/assets/libs/font-awesome/3.2.1/css/font-awesome{% if not sys_param('debug')|default(false) %}.min{% endif %}.css?ver={{ver|default(1)}}">
	{% block sub_css_block %}{% endblock %}
{% endblock %}

{% block js_block %}
	{#<script type="text/javascript" src="//cdn.bootcss.com/jquery/3.1.1/core.js"></script> #}
	<script type="text/javascript" src="/assets/libs/jquery/3.1.1/jquery{% if not sys_param('debug')|default(false) %}.min{% endif %}.js?ver={{ver|default(1)}}"></script>
	<script type="text/javascript" src="/assets/libs/jquery-validate/1.15.1/jquery.validate{% if not sys_param('debug')|default(false) %}.min{% endif %}.js?ver={{ver|default(1)}}"></script>
	<script type="text/javascript" src="/assets/libs/bootstrap/4.0.0-alpha.5/js/bootstrap{% if not sys_param('debug')|default(false) %}.min{% endif %}.js?ver={{ver|default(1)}}"></script>
	<script type="text/javascript" src="/assets/libs/bootstrap-notify/3.1.3/bootstrap-notify{% if not sys_param('debug')|default(false) %}.min{% endif %}.js?ver={{ver|default(1)}}"></script>
	<script type="text/javascript" src="/assets/js/base.js?ver={{ver|default(1)}}"></script>
	{% block sub_js_block %}{% endblock %}
{% endblock %}